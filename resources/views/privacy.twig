{% extends isPjax is defined and isPjax == true ? "pjax.twig" : "theme.twig" %}
{% block title -%}
	{{ trans('general.privacy.title') }}
{%- endblock %}
{% block description -%}
	{{ trans('general.privacy.description') }}
{%- endblock %}
{% block content %}
		<article class="privacy">
			<section class="heading">
				<h1>{{ trans('general.privacy.title') }}</h1>
			</section>

			<section class="content">

				<div class="container-fluid">

					<div class="row">
						<div class="col col-12">
							{{ trans('general.privacy.content') | markdown }}
						</div>
					</div>

				</div>

			</section>
		</article>
{% endblock %}
